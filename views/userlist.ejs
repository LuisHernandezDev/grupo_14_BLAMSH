<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>

    <% users.forEach(users=> { %>
        <div class=usuario>
            <h1>
                <%= users.id %>
            </h1>
            <h1>
                <%= users.first_name %>
            </h1>
            <h1>
                <%= users.last_name %>
            </h1>
            <h1>
                <%= users.email%>
            </h1>
            <h1>
                <%= users.password%>
            </h1>
            <h1>
                <%= users.gender%>
            </h1>
            <h1>
                <%= users.birthday_date%>
            </h1>
            <h1>
                <%=users.phone%>
            </h1>
            <h1>
                <%=users.image%>
            </h1>

        </div>
        <% }) %>

</body>

</html>